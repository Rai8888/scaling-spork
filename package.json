  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^5.6.1",
    "axios": "^1.4.0",
    "bootstrap": "^5.3.0",
    "dayjs": "^1.11.8",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-router-dom": "^6.12.1",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.13.8",
